name        = mango
description = A theme based on Radix.
screenshot  = screenshot.png
core        = 7.x

; Base - Radix
base theme  = radix

; Regions
regions[jumbotron] = Jumbotron
regions[content] = Content

; Breakpoints
breakpoints[xs] = (max-width: 767px)
breakpoints[sm] = (min-width: 768px) and (max-width: 991px)
breakpoints[md] = (min-width: 992px) and (max-width: 1199px)
breakpoints[lg] = (min-width: 1200px)

; Panels
plugins[panels][layouts] = panels/layouts
plugins[panels][styles] = panels/styles

; CSS - General
stylesheets[all][]     = assets/css/mango.style.css

; Javascript
scripts[] = assets/js/mango.script.js

; Settings
; Set this to FALSE for development only.
settings[preprocess_css] = FALSE

; Information added by Drupal.org packaging script on 2016-05-15
version = "7.x-3.4"
core = "7.x"
project = "radix"
datestamp = "1463294557"

